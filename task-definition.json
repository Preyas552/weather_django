{
  "family": "weather-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::533267320522:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "weather-container",
      "image": "533267320522.dkr.ecr.ca-central-1.amazonaws.com/weather-app:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "entryPoint": ["python"],
      "command": ["app.py"]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}